{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 450,
    "title": "Airports In Australia",
    "projection": {"type": "equalEarth"},
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/Nicolelim888/FIT3179/main/ne_110m%20(4).json",
          "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}
        },
        "mark": {"type": "geoshape", "fill": "lightblue", "stroke": "white"}
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/Nicolelim888/FIT3179/main/airport_volume_airport_locations.csv"
        },
        "mark": {"type": "circle", "tooltip": {"content": "data"}},
        "encoding": {
          "longitude": {"field": "Airport1Longitude", "type": "quantitative"},
          "latitude": {"field": "Airport1Latitude", "type": "quantitative"},
          "size": {
            "field": "TotalSeats",
            "type": "quantitative",
            "title": "Total seats in airport",
            "scale": {"domain": [500, 200000]}
          }
        }
      }
    ]
  }